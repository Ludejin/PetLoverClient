<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="Markable">
        <attr name="mark" format="reference"/>
        <attr name="mark1" format="reference"/>
        <attr name="mark2" format="reference"/>
        <attr name="mark3" format="reference"/>
        <attr name="markVisible" format="boolean"/>
        <attr name="markVisibleWhenSelected" format="boolean"/>
        <attr name="markWidth" format="dimension|reference"/>
        <attr name="markHeight" format="dimension|reference"/>
        <attr name="markOffsetX" format="dimension|reference"/>
        <attr name="markOffsetY" format="dimension|reference"/>
        <attr name="markPosition" format="integer">
            <flag name="center" value="1"/>
            <flag name="left_top" value="2"/>
            <flag name="right_top" value="3"/>
            <flag name="right_bottom" value="4"/>
            <flag name="left_bottom" value="5"/>
        </attr>
        <attr name="markType" format="integer">
            <flag name="show_none" value="1"/>
            <flag name="show_point" value="2"/>
            <flag name="show_text" value="3"/>
            <flag name="show_number" value="4"/>
        </attr>
        <attr name="markTextSize" format="dimension|reference"/>
        <attr name="markTextColor" format="color|reference"/>
        <attr name="markAutoPadding" format="boolean"/>
    </declare-styleable>
</resources>